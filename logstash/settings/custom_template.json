{
	"template": "logstash-*",
	"settings": {
		"number_of_shards" : "1",
		"number_of_replicas" : "0"
	},
	"mappings": {
		"properties" : {
        	"@timestamp" : {
            	"type" : "date"
          	},
        	"@version" : {
            	"type" : "keyword"
          	},
        	"host" : {
            	"type" : "ip"
          	},
        	"message" : {
            	"type" : "keyword"
          	}
        }
	}
}